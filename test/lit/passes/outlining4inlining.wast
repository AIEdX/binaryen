;; RUN: foreach %s %t wasm-opt --inlining --all-features -S -o - | filecheck %s

(module
  (import "out" "func" (func $import))

  (func $maybe-work-hard (param $x i32)
    (if
      (i32.eqz
        (local.get $x)
      )
      (return)
    )
    (loop $l
      (call $import)
      (br $l)
    )
  )
)
